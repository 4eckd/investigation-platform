    "@semantic-release/github": "^11.0.3",
    "@semantic-release/npm": "^12.0.2",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
